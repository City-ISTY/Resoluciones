    {
      "parameters": {
        "jsCode": "// Preprocesar la lista de integrantes\n\nconst integrantesHTML = $input.first().json.attendees.map(integrante => {\n  return `<p style=\"text-align: left;\">${integrante.name}<br><strong>${integrante.role}</strong></p>`;\n}).join('');\n// Preprocesar los temas de la reunión\nlet temasHTML = '';\nconst temas = $input.first().json.meeting_topics\nconst newTopic = \"Temas Varios.\";\n\n// Iniciar la lista\ntemasHTML += '';\n\n// Agregar todos los temas existentes\ntemas.forEach((tema, index) => {\n  let numeroTema = index + 2;\n  temasHTML += `<li>${numeroTema}. ${tema.topic}</li>`;\n  \n  // Subtemas como sub-lista\n  if (tema.sub_topics && tema.sub_topics.length > 0) {\n    temasHTML += '<ul>';\n    tema.sub_topics.forEach(subtema => {\n      temasHTML += `<li>${subtema}</li>`;\n    });\n    temasHTML += '</ul>';\n  }\n});\n\n// Validar si es reunión ordinaria (case insensitive)\nif ($input.first().json.meeting_type.toLowerCase().includes('ordinaria')) {\n  const numeroUltimoTema = temas.length + 2;\n  temasHTML += `<li>${numeroUltimoTema}. ${newTopic}</li>`;\n}\n\n\n// Agregar el HTML generado al objeto principal\nreturn {\n  json: {\n    ...items[0].json,\n    integrantesHTML: integrantesHTML,\n    temasHTML: temasHTML\n  }\n};"
      },
